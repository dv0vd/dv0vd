[Definition]
failregex = ^<HOST> - \S+ \[\] "[^"]*" 404

datepattern = {^LN-BEG}%%ExY(?P<_sep>[-/.])%%m(?P=_sep)%%d[T ]%%H:%%M:%%S(?:[.,]%%f)?(?:\s*%%z)?
              ^[^\[]*\[({DATE})
              {^LN-BEG}

journalmatch = _SYSTEMD_UNIT=nginx.service + _COMM=nginx

ignoreregex = ^<HOST>.*"GET /(?:_matrix|_synapse/client|\.well-known/matrix/client).*"

